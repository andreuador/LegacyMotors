{{ form_start(form, {'attr': {'class': 'row g-3'}}) }}
<div class="row">
    <div class="col-md-6">
        {{ form_label(form.start_date, 'Fecha inicio') }}
        {{ form_widget(form.start_date, {'attr': {'class': 'form-control'}}) }}
    </div>
    <div class="col-md-6">
        {{ form_label(form.end_date, 'Fecha fin') }}
        {{ form_widget(form.end_date, {'attr': {'class': 'form-control'}}) }}
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        {{ form_label(form.total_price, 'Precio total') }}
        {{ form_widget(form.total_price, {'attr': {'class': 'form-control'}}) }}
    </div>
    <div class="col-md-6">
        {{ form_label(form.vehicle, 'Veh√≠culo') }}
        {{ form_widget(form.vehicle, {'attr': {'class': 'form-control'}}) }}
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        {{ form_label(form.paymentDetails, 'Detalles de Pago') }}
        {{ form_widget(form.paymentDetails, {'attr': {'class': 'form-control'}}) }}
    </div>
    <div class="col-md-6">
        {{ form_label(form.customer, 'Cliente') }}
        {{ form_widget(form.customer, {'attr': {'class': 'form-control'}}) }}
    </div>
</div>
<div class="col-12 my-2">
    <button type="submit" class="btn btn-success">{{ button_label|default('Guardar') }}</button>
</div>
{{ form_end(form) }}
